{
	"extends": "../../tslint.json",
	"rules": {
		"no-empty": false,
		"no-output-native": false,
		"no-unused-expression": false,
		"directive-selector": [
			false,
			"attribute",
			"lib",
			"camelCase"
		],
		"component-selector": [
			false,
			"element",
			"lib",
			"kebab-case"
		],
		"no-host-metadata-property": false
	}
}